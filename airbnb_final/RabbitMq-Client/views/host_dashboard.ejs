<div class="col-lg-4" style="width:450px; height:350px;padding-left:4cm">
    <br /><br /><br /><br />
<div class="ct4-chart ct-perfect-fourth" style="width:410px;height:400px;"></div>
  <script type="text/javascript">
var data = {
  labels: [<% for(i in areacount){ %>
                                   "<%- areacount[i].city %>",
                                <%  } %>],
  series: [<% for(i in areacount){ %>
                                   "<%- areacount[i].sum_count %>",
                                <%  } %>]
};

var options = {
  labelInterpolationFnc: function(value) {
    return value[0]
  }
};

var responsiveOptions = [
  ['screen and (min-width: 640px)', {
    chartPadding: 30,
    labelOffset: 100,
    labelDirection: 'explode',
    labelInterpolationFnc: function(value) {
      return value;
    }
  }],
  ['screen and (min-width: 1024px)', {
    labelOffset: 80,
    chartPadding: 20
  }]
];

new Chartist.Pie('.ct4-chart', data, options, responsiveOptions);
</script>
